{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "plugin:react/recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "react/default-props-match-prop-types": "error",
    "react/destructuring-assignment": "error",
    "react/require-default-props": "error"
  }
}
